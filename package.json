{
  "name": "rt90-admin",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "build": "webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "precommit": "lint-staged",
    "submodule-init": "git submodule update --init",
    "submodule-pull": "git submodule foreach git pull origin master",
    "submodule-commit": "git submodule foreach git add --all && git submodule foreach git commit -m",
    "submodule-push": "git submodule foreach git push origin HEAD:master"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --single-quote --trailing-comma all --print-width 120",
      "tslint --project ./tsconfig.json -s node_modules/custom-tslint-formatters/formatters -t grouped",
      "git add"
    ],
    "*.pug": [
      "pug-lint -c .pug-lintrc.json"
    ],
    "*.scss": [
      "scss-lint -c .scss-lint.yml"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/cdk": "^5.1.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.1.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "apollo-angular": "^1.0.0",
    "apollo-angular-link-http": "^1.0.1",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-error": "^1.0.6",
    "apollo-link-ws": "^1.0.6",
    "core-js": "^2.4.1",
    "graphql": "^0.11.2",
    "graphql-tag": "^2.6.1",
    "hammerjs": "^2.0.8",
    "ng-pick-datetime": "^5.2.6",
    "ngx-auto-unsubscribe": "^2.3.0",
    "ngx-quill-editor": "^2.2.2",
    "pug-lint": "^2.5.0",
    "rxjs": "^5.5.6",
    "scss-lint": "0.0.0",
    "subscriptions-transport-ws": "^0.9.7",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/graphql": "^0.12.4",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "apply-loader": "^2.0.0",
    "autoprefixer": "^7.2.3",
    "circular-dependency-plugin": "^4.2.1",
    "codelyzer": "^4.0.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "custom-tslint-formatters": "2.1.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.1.2",
    "prettier": "^1.10.2",
    "protractor": "~5.1.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.11.0"
  }
}
